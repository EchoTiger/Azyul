				</SetMailbox>
				<CustomBehavior File="WaitTimer" WaitTime="1500" />
				<CustomBehavior File="ForceSetVendor" DoMail="True" DoSell="True" DoRepair="True" />
				<MoveTo Nav="Run" X="-2936.408" Y="4010.14" Z="-1.248712" />
