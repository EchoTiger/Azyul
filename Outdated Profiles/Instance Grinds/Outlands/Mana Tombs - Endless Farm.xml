			<RunTo X="-253.0494" Y="-162.5546" Z="-0.8904037" />
			<RunTo X="-297.0168" Y="-203.995" Z="-1.683555" />
			<RunTo X="-327.7555" Y="-225.4176" Z="-0.9572976" />
			<RunTo X="-356.417" Y="-223.2754" Z="-0.9587091" />
