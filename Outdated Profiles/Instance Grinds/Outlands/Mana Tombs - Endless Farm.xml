				<MoveTo Nav="Fly" X="-3521.181" Y="4939.856" Z="-101.3959" />
				<SetVendor>
					<Vendors>
						<Vendor Name="Provisioner Tsaalt" Entry="19718" Type="Food" X="-3523.476" Y="4937.63" Z="-101.3944" />
