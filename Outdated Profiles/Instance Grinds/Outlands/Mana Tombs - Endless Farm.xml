	<!-- Vendor Routine -->
	<If Condition="(Me.FreeBagSlots &lt;= 10)" >
		<CustomBehavior File="Message" Text="Moving to Sell, Repair, Mail" LogColor="Lime" />
		<CustomBehavior File="Misc\RunLua" Lua="vendorstartingmoney = GetMoney();" />

